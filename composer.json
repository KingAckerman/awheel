        },
        {
            "type": "vcs",
            "url": "git@git.fengqi.tk:/light/view-blade.git"
        },
        {
            "type": "vcs",
            "url": "git@git.fengqi.tk:/light/view-smarty.git"
        }
    ],
    "config": {
        "prefer-source": false,
        "prefer-dist": true,
        "secure-http": false
